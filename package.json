{
  "name": "mmc-ui-react",
  "version": "1.0.0",
  "description": "mmcuav ui library for react",
  "main": "index.js",
  "keywords": [
    "react",
    "ui",
    "component"
  ],
  "files": [
    "dist/*",
    "lib",
    "index.js",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist && rimraf lib",
    "build:tsc": "tsc -p tsconfig.build.json --target ES5 --outDir lib",
    "build:es": "cpr src/styles lib/styles -o && cpr src/assets lib/assets -o",
    "build": "npm run clean && npm run build:prod && npm run build:dev && npm run build:tsc && npm run build:es"
  },
  "author": "chars",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.3.1",
    "docz": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@emotion/core": "10.1.1",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "javascript-obfuscator": "^4.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-obfuscator": "^3.5.1"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  }
}
